meta {
  name: Hello World Test
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}
}

assert {
  res.status: eq 200
  res.body: eq Hello World2!
}

tests {
  test("Response should be Hello World2!", function() {
    expect(res.body).to.equal("Hello World2!");
  });
  
  test("Response time should be less than 100ms", function() {
    expect(res.responseTime).to.be.below(100);
  });
}