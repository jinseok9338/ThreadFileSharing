meta {
  name: Logout and revoke token
  type: http
  seq: 6
}

post {
  url: {{baseUrl}}/api/v1/auth/logout
  body: json
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

body:json {
  {
    "refreshToken": "{{refreshToken}}"
  }
}

assert {
  res.status: eq 200
  res.body.message: eq Logged out successfully
}

tests {
  test("Should revoke refresh token", function() {
    expect(res.status).to.equal(200);
  });
}

